@model IEnumerable<InfoCenterMVC.Models.MvcAsteriskCallModel>

@{
    ViewBag.Title = "Asterisk Callers";
}

<h2>Вызовы в Asterisk</h2>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Caller)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Callee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_Time)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Caller)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Callee)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date_Time)
                </td>
            </tr>
        }
    </tbody>
</table>

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
